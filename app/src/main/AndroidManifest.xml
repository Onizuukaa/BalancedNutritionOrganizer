<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hfad.com.balancednutritionorganizer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DailyMealsActivity"></activity>
        <activity
            android:name=".YourDishesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ComposingDishesActivity"


            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BmiActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".AdvancedInformationAboutProductActivity" />
        <activity android:name=".FruitsListActivity"
            android:parentActivityName=".ProductTableActivity"/>
        <activity android:name=".VegetablesListActivity"
            android:parentActivityName=".ProductTableActivity"/>
        <activity
            android:name=".ProductTableActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>